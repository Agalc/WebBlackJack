@{
  ViewBag.Title = "Hand";
}
@using BlackJack.Core.Enteties
@model BlackJack.Core.Enteties.User
<body>
  <div>
    <h2>Hand</h2>
    <table>
      <tr>
        <td><p>Suit</p></td>
        <td><p>Face</p></td>
      </tr> 
      @foreach (Card o in Model.Cards)
      {
        <tr>
          <td><p>@o.Suit</p></td>
          <td><p>@o.Face</p></td>
        </tr>
      }
    </table>
  </div>
</body>